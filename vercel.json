{
  "version": 2,
  "builds": [
    {
      "src": "crces-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/crces-frontend/$1"
    }
  ],
  "buildCommand": "cd crces-frontend && npm install && npm run build",
  "outputDirectory": "crces-frontend/dist",
  "framework": "vite"
}

